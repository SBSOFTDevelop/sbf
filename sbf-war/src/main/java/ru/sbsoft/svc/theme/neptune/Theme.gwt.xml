<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN"
    "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module>
  <inherits name="ru.sbsoft.svc.themebuilder.base.ThemeBase"/>

  <extend-property name="svc.theme" values="neptune"/>
  <set-property name="svc.theme" value="neptune"/>

  <!-- Constants decl - generated instance used to build theme -->
  <replace-with class="ru.sbsoft.svc.theme.neptune.client.ThemeDetailsImpl">
    <when-type-is class="ru.sbsoft.svc.themebuilder.base.client.config.ThemeDetails"/>
    <when-property-is name="svc.theme" value="neptune"/>
  </replace-with>

  <!-- Default is Sliced+Css3.  use slicedDisabled flag to use only Css3 version -->
  <inherits name="ru.sbsoft.svc.theme.neptune.Sliced" />
</module>
