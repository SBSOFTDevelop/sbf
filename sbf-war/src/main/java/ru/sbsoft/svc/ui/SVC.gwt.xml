<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN"
    "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module>
  <inherits name="ru.sbsoft.svc.data.Data" />
  <inherits name="ru.sbsoft.svc.dnd.core.DND" />
  <inherits name="ru.sbsoft.svc.state.State" />
  <inherits name="ru.sbsoft.svc.widget.core.Core" />

  <!-- Default theme is Blue -->
  <inherits name="ru.sbsoft.svc.theme.blue.Blue" />

  <!-- Collect OS, collapse it by default -->
  <collapse-property name="user.agent.os" values="*" />
  <collapse-property name="svc.user.agent" values="air, safari*, chrome" />
  <collapse-property name="svc.user.agent" values="gecko*" />
</module>